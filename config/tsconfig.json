{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": ["es2015", "dom"],
    "types": [
      "node",
      "jasmine"
    ],
    "noImplicitAny": true,
    "removeComments": true,
    "noEmitOnError": true
  }
}
